<%- definitions.each do |name, definition| %>
if (<%= prefix %>.<%= name %> != null<%= string?(definition) ? " && !#{prefix}.#{name}.equals(\"<null>\")" : "" %>) {
    <%- if nested?(definition) %>
<%= render_converter(name, definition) %>
    <%- else %>
    row.set("<%= field_name(name) %>", <%= value(name, definition) %>);
    <%- end %>
}
<%- end %>
